{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/workouts/(.*)",
      "dest": "/api/workouts/$1"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "/api/user/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
